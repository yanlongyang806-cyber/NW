OperationDef
	Name "Comparison_LessThan" // Name of the operation
	Category "Combiner"
	ParentName "Comparison"
	OptionName "Less than"
	Description "For each channel, returns 1 if A is less than B"
	Type SIMPLE

	Input: A, "First parameter"
	Input: B, "Second parameter"

	Output
		Name Result
		Description "For each channel, returns 1 if A is less than B"
		Float 4
		AlphaFrom B
	EndOutput

EndOperationDef

