OperationDef
	Name "TextureNormalDXT5nm_7" // Name of the operation
	ParentName "TextureNormalDXT5nm_2+"
	OptionName "7 texture coords"
	Description "Texture fetch"
	Type TEXTURE

	Input
		Name TexCoord
		Description "Texture Coordinates"
		Float 2
		Type TexCoord
		Default
			Type TexCoord0
		EndDefault
	EndInput
