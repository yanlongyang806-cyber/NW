		EndDefault
	EndInput
	
	Input
		Name NormalMap // String input in editor, integer texture unit number output to shader
		Description "Texture name"
		Type TextureNormal
		Default
			Strings "default_dxt5nm_N"
		EndDefault
		Texture 1
	EndInput

	Input
		Name AlphaIfDXT5nm
		Description "Alpha to use if this is a DXT5nm texture"
		NoAutoConnect 1
		NoManualConnect 1
		Float 1
		FloatRange 0 1
		Default
			Floats 1
		EndDefault
	EndInput

	Input
		Name IsDXT5nm
		Description "Set to 1 if this is a DXT5nm texture"
		NoAutoConnect 1
		NoManualConnect 1
		Hidden 1
		Type TextureNormal_IsDXT5nm
		Float 1
	EndInput
	
	Output
		Name Result
		Description "Normal from the texture"
		Float 4
		Type Normal
		AlphaFrom NormalMap
	EndOutput
